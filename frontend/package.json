{
  "name": "m62",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev:frontend": "react-router dev",
    "dev:backend": "cd ../backend && uvicorn main:app --reload --host 0.0.0.0 --port 3000",
    "dev": "concurrently --kill-others \"npm:dev:frontend\" \"npm:dev:backend\"",
    "start": "react-router-serve ./build/client/",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 10",
    "format": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@react-router/node": "^7.12.0",
    "@react-router/serve": "^7.12.0",
    "d3": "^7.9.0",
    "isbot": "^5.1.31",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-icons": "^5.5.0",
    "react-router": "^7.12.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.12.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/d3": "^7.4.3",
    "@types/node": "^22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
